def circle_circumference(radius):
    pi = 3.147
    circumference = 2 * pi * radius
    print(circumference, "is the circumference of the circle")
def circle.area(radius):
    area = 2 * radius * radius
    print(area, "is the area of the circle")
def square_area(side):
    area = side * side
    print(area, "is the area of the square")
def square_perimeter(side):
    perimeter = 4 * side
    print(perimeter,"is the perimeter of the square")
def rectangle_area(side1,side2):
    area = side1 * side2
    print(area,"is the is the area of the rectangle")
def rectangle_perimeter(side1,side2):
    perimeter = (side1 * 2) + (side2 * 2)
    print(perimeter,"is the is the perimeter of the rectangle")
